<mvc:View controllerName="zerotouch.controller.Main" displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:u="sap.ui.unified"
	xmlns:core="sap.ui.core"
	xmlns:table="sap.ui.table">
	<Page id="_IDGenPage1" >
		<Panel id="_IDGenPanel1" class="sapUiResponsiveMargin" expandable="false" width="auto" headerText="Consolidated Adhoc Finance Interface - Submit Invoice For Payment without Purchase Order">
			<VBox id="_IDGenVBox1" alignContent="Stretch" class="sapUiResponsiveMargin" displayInline="true" fieldGroupIds="checkingReq">
				<HBox id="_IDGenHBox1">
					<Label id="_IDGenLabel1" text="Budget Holder" showColon="true" required="true" labelFor="_IDGenInput1" class="sapUiTinyMarginBegin sapUiTinyMarginTop" width="140px" wrapping="true"/>
					<Input id="_IDGenInput1" class="sapUiTinyMarginBegin sapUiSmallMarginEnd" editable="false" />
					<Label id="_IDGenLabel2" text="Approver" showColon="true" required="true" labelFor="_IDGenInput2" class="sapUiMediumMarginBegin sapUiTinyMarginTop"/>
					<Input id="_IDGenInput2" class="sapUiTinyMarginBegin" editable="false" />
					<Input id="_BgtMngrEmail" visible="false" />
					<items/>
				</HBox>
				<HBox id="_IDGenHBox2">
					<Label id="_IDGenLabel3" text="Reason" showColon="true" required="true" labelFor="_IDGenComboBox1" class="sapUiTinyMarginBegin sapUiTinyMarginTop" width="140px"/>
					<ComboBox id="_IDGenComboBox1" class="sapUiTinyMarginBegin" width="500px" required="true" change="onReasonChange" items="{excelData>/reasonDropdown}" valueStateText="Enter a valid reason" editable="{= ${excelData>/Parameters/mode} !== 'display'}" >
						<core:Item key="{excelData>ReasonCode}" text="{excelData>ReasonDesc}" />
					</ComboBox>
					<Label id="_IDGenLabel4" text="Submit Date" class="sapUiMediumMarginBegin sapUiSmallMarginTop sapUiMediumMarginEnd" labelFor="_IDGenDatePicker2"/>
					<Label id="_IDGenLabel5" text="Approve Date" class="sapUiLargeMarginBegin sapUiSmallMarginTop" labelFor="_IDGenDatePicker3"/>
				</HBox>
				<HBox id="_IDGenHBox3">
					<Label id="_IDGenLabel6" text="Invoice Attachment" showColon="true" required="true" labelFor="_IDGenLink1" class="sapUiTinyMarginBegin sapUiTinyMarginTop" width="140px"/>
					<!-- <Input id="" class="sapUiTinyMarginBegin" width="500px" required="true" change="onAttachChange" value="Hi there" valueStateText="Enter a Invoice Attachment" editable="false" /> -->
					<Link id="_IDGenLink1" target="_blank" text="No Attachment" width="500px" class="sapUiTinyMarginTop sapUiTinyMarginStart" enabled="false" />
					<DatePicker id="_IDGenDatePicker1" class="sapUiSmallMarginBegin sapUiSmallMarginEnd" displayFormat="dd.MM.yyyy" width="140px" placeholder="Select Date" editable="false"/>
					<DatePicker id="_IDGenDatePicker2" class="sapUiTinyMarginBegin" width="140px" displayFormat="dd.MM.yyyy" placeholder="Select Date" editable="false"/>
				</HBox>
				<CheckBox id="_IDGenCheckBox1" class="sapUiTinyMarginBegin" editable="{= ${excelData>/Parameters/mode} !== 'display'}" text="Transactions entered comply with the guidelines for processing payments via this interface and could not be processed via SAP purchase orders"/>
				<HBox id="_IDGenHBox4" justifyContent="Center">
					<Label id="_IDGenLabel7" text="Total (ex-VAT)" showColon="true" labelFor="_IDGenInput5" class="sapUiTinyMarginTop"/>
					<Input id="_IDGenInput3" width="80px" class="sapUiTinyMarginBegin sapUiTinyMarginEnd" editable="false" required="true" />
					<Label id="_IDGenLabel8" text="Tax" showColon="true" labelFor="_IDGenInput6" class="sapUiTinyMarginTop"/>
					<Input id="_IDGenInput4" width="80px" class="sapUiTinyMarginBegin sapUiTinyMarginEnd" editable="false" required="true" />
					<Label id="_IDGenLabel9" text="Total" showColon="true" labelFor="_IDGenInput7" class="sapUiTinyMarginTop"/>
					<Input id="_IDGenInput5" width="80px" class="sapUiTinyMarginBegin" editable="false" required="true" />
				</HBox>
			</VBox>
		</Panel>
		<Table id="Table" items="{excelData>/items}" width="auto" class="sapUiResponsiveMargin" mode="MultiSelect" selectionChange="_onSelectionChange" updateFinished="_onTableUpdateFinished" backgroundDesign="Solid" contextualWidth="auto" growingDirection="Downwards" growing="true" growingScrollToLoad="true" showSeparators="Inner"  fixedLayout="true" >
			<headerToolbar>
				<Toolbar id="_IDGenToolbar1" width="auto" design="Solid" style="Standard" >
					<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
					<u:FileUploader id="_IDGenFileUploader1" visible="{= ${excelData>/Parameters/mode} !== 'display'}" iconOnly="false" buttonOnly="true" fileType="XLSX,xlsx" icon="sap-icon://upload" iconFirst="true" style="Emphasized" change="_onExcelUpload" sameFilenameAllowed="true" uploadComplete="_onExcelComplete" buttonText="Load" useMultipart="false" multiple="false" uploadUrl="/sap/opu/odata/sap/ZRTRGTGW_CONS_INV_SRV/zrtgtgw_ci_fileSet" typeMissmatch="onFileTypeMismatch"  />
					<Button id="_IDGenButton1" text="Add Row" visible="{= ${excelData>/Parameters/mode} !== 'display'}" type="Emphasized" press="_AddRow" icon="sap-icon://add"/>
					<Button id="_IDGenButton2" text="Delete Row" visible="{= ${excelData>/Parameters/mode} !== 'display'}" type="Emphasized" press="_DelRow" icon="sap-icon://delete"/>
				</Toolbar>
			</headerToolbar>
			<columns>
				<Column id="_IDGenColumn1" hAlign="Center" width="90px" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText1" text="Vendor"/>
				</Column>
				<Column id="_IDGenColumn2" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText2" text="Vendor Name"/>
				</Column>
				<Column id="_IDGenColumn3" hAlign="Center" width="110px" demandPopin="true" minScreenWidth="Tablet" >
					<Text id="_IDGenText3" text="Invoice Date"/>
				</Column>
				<Column id="_IDGenColumn4" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText4" text="Reference"/>
				</Column>
				<Column id="_IDGenColumn5" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText5" text="Cost Center/WBS/Internal Order"/>
				</Column>
				<Column id="_IDGenColumn6" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText6" text="Cost Centre Description"/>
				</Column>
				<Column id="_IDGenColumn7" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText7" text="Budget Manager"/>
				</Column>
				<Column id="_IDGenColumn8" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText8" text="Ledger Code"/>
				</Column>
				<Column id="_IDGenColumn9" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText9" text="General Ledger Description"/>
				</Column>
				<Column id="_IDGenColumn10" hAlign="Center" width="auto" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText10" text="Item Value (excl VAT)"/>
				</Column>
				<Column id="_IDGenColumn11" hAlign="Center" width="50px" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText11" text="Tax Code"/>
				</Column>
				<Column id="_IDGenColumn12" hAlign="Center" width="50px" demandPopin="true" minScreenWidth="Tablet" visible="false">
					<Text id="_IDGenText12" text="Tax Amount"/>
				</Column>
				<Column id="_IDGenColumn13" hAlign="Center" width="150px" demandPopin="true" minScreenWidth="Tablet">
					<Text id="_IDGenText13" text="Item Description"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem id="_IDGenColumnListItem1">
				<customData>
					<core:CustomData key="excelData" value="{excelData>Vendor}" writeToDom="true" />
				</customData>
					<!-- <Input id="" type="Number" value="{excelData>Vendor}" editable="false" width="auto"/><Input id="" value="{excelData>VendorName}" editable="false" width="auto"/><Input id="" value="{excelData>Reference}" editable="false" width="auto"/><Input id="" type="Number" value="{excelData>CostCentre}" editable="false" width="auto"/><Input id="" value="{excelData>CcName}" editable="false" width="auto"/><Input id="" value="{excelData>BudgetManager}" editable="false" width="auto"/><Input id="" type="Number" value="{excelData>GlCode}" editable="false" width="auto"/><Input id="" value="{excelData>GlName}" editable="false" width="auto"/><Input id="" type="Number" value="{excelData>ItemValue}" editable="false" width="auto"/><Input id="" value="{excelData>Vat}" editable="false" width="auto"/><Input id="" value="{excelData>ItemDesc}" editable="false" width="auto"/> -->
					<!-- // text -->
					<Text id="_IDGenText14" text="{excelData>Vendor}" width="auto"/>
					<Text id="_IDGenText15" text="{excelData>VendorName}"  width="auto"/>
					<DatePicker id="_IDGenDatePicker3" value="{excelData>InvoiceDate}" editable="false" width="auto" displayFormat="dd.MM.yyyy" class="removeBG" />
					<!-- <Text id="" text="{excelData>InvoiceDate}"  width="auto"/> -->
					<Text id="_IDGenText16" text="{excelData>Reference}"  width="auto"/>
					<Text id="_IDGenText17" text="{excelData>Cciowbs}"  width="auto"/>
					<Text id="_IDGenText18" text="{excelData>CciowbsName}"  width="auto"/>
					<Text id="_IDGenText19" text="{excelData>BudgetManager}"  width="auto"/>
					<Text id="_IDGenText20" text="{excelData>GlCode}"  width="auto"/>
					<Text id="_IDGenText21" text="{excelData>GlName}"  width="auto"/>
					<Text id="_IDGenText22" text="{excelData>ItemValue}"  width="auto"/>
					<Text id="_IDGenText23" text="{excelData>Vat}"  width="auto"/>
					<Text id="_IDGenText24" text="{excelData>VATAmount}"  width="auto"/>
					<Text id="_IDGenText25" text="{excelData>ItemDesc}"  width="auto"/>
				</ColumnListItem>
			</items>
		</Table>
		<footer>
			<OverflowToolbar id="_IDGenOverflowToolbar1">
				<Button id="_IDGenButton3" text="Attachments" type="Emphasized" icon="sap-icon://attachment" press="onAttach" visible="{= ${excelData>/Parameters/mode} !== 'display'}" />
				<ToolbarSpacer id="_IDGenToolbarSpacer2"/>
				<Button id="_IDGenButton4" text="Submit" type="Emphasized" icon="sap-icon://accept" press="onSubmit" visible="{= ${excelData>/Parameters/mode} !== 'display'}" />
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>
