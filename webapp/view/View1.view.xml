<mvc:View controllerName="com.gcc.eforms.ex01.ex01.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" displayBlock="true"
    xmlns="sap.m">
    <Page id="page">
        <content>
         <IconTabBar id="_IDGenIconTabBar1" backgroundDesign="Solid" expandable="true" select="onTabChange">
            <items>
               <IconTabFilter id="_IDGenIconTabFilter1" text="{i18n>FormText}" key="i18n>FormText">
                  <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
                     <!--Begin of INTRODUCTION-->
                     <Panel id="_IDGenPanel1" expandable="true" expanded="true" stickyHeader="true" headerText="{i18n>Intro}" width="auto" class="sapUiResponsiveMargin">
                        <HBox id="HBOX01">
                           <!-- <Text id="text01" text="{i18n>IntroText1}" renderWhitespace="true" wrappingType="Normal" />
                           <Link id="link01" text="{i18n>Link}" href="{i18n>introLink}" class="sapuiTinyMarginBegin sapuiTinyMarginTop" target="_blank" /> -->
                           <FormattedText id="_IDGenFormattedText1" ></FormattedText>
                        </HBox>
                     </Panel>
                     <!-- End Of Introduction -->
                     <!-- Begin Of Header -->
                     <Panel id="_IDGenPanel2" expandable="true" expanded="true" stickyHeader="true" headerText="{i18n>Head}" width="auto" class="sapUiResponsiveMargin">
                        <VBox id="VBOX02">
                           <!-- INITIATOR, FORM ID, DATE -->
                           <HBox id="HBOX02">
                              <!-- INITIATOR -->
                              <Label id="_IDGenLabel1" text="{i18n>Initiator}" showColon="true" labelFor="_IDGenInput1" width="80px" class="sapUiSmallMarginTop" />
                              <Input id="_IDGenInput1" width="200px" editable="false" value="{Model1>/initNameP/salutationLabel} {Model1>/initNameP/firstNameInit} {Model1>/initNameP/lastNameInit}" class="sapUiTinyMarginTop sapUiSmallMarginEnd" />
                              <!-- INITIATOR -->
                              <!-- FORM ID -->
                              <Label id="_IDGenLabel2" text="{i18n>Form}" showColon="true" labelFor="_IDGenInput2" width="80px" class="sapUiSmallMarginTop sapUiMediumMarginBegin" />
                              <Input id="_IDGenInput2" width="140px" editable="false" class="sapUiTinyMarginTop sapUiSmallMarginEnd" />
                              <!-- FORM ID -->
                              <!-- DATE -->
                              <Label id="_IDGenLabel3" text="Date" showColon="true" width="80px" labelFor="DatePicker01" class="sapUiSmallMarginTop sapUiMediumMarginBegin" />
                              <DatePicker id="DatePicker01" editable="false" class="sapUiTinyMarginTop" width="150px" />
                              <!-- DATE -->
                           </HBox>
                           <!-- INITIATOR, FORM ID, DATE -->
                           <!-- ORG NAME, COST CENTRE -->
                           <HBox id="HBOX03" class="sapUiTinyMarginTop">
                              <Label id="_IdGenLabel6" text="Organisation Name" showColon="true" width="4cm" class="sapUiSmallMarginTop sapUiSmallMarginBottom" labelFor="_IdGenInput6" />
                              <ComboBox id="_IdGenInput6" value="{Model1>/OrgNameP/customString3}" class="sapUiTinyMarginTop sapUiMediumMarginEnd" width="6cm" items="{path: 'Model1>/OrgValues', templateShareable:false}" change="onOrgChange" enabled="{= ${Model1>/Param/mode} !== 'display'}" >
                                 <items>
                                    <core:Item key="{Model1>key}" text="{Model1>value} ({Model1>key})" />
                                 </items>
                              </ComboBox>
                              <Label id="_IdGenLabel5" text="Cost Centre" width="3.5cm" showColon="true" labelFor="_IdGenInput5" class="sapUiSmallMarginTop sapUiLargeMarginBegin sapUiSmallMarginBottom" />
                              <Input id="_IdGenInput5" editable="false" class="sapUiTinyMarginTop sapUiTinyMarginEnd" value="{Model1>/CostCentreP/costCenter}" width="4cm" />
                           </HBox>
                           <!-- ORG NAME, COST CENTRE -->
                           <!-- CLAIM MONTH, CLAIM END DATE -->
                           <HBox id="HBOX04" class="sapUiTinyMarginTop">
                              <Label id="_IdGenLabel9" text="Claim Month" showColon="true" required="true" width="4cm" class="sapUiTinyMarginTop" labelFor="_IDGenComboBox1" />
                              <ComboBox id="_IDGenComboBox1" editable="{= ${Model1>/form/mode} !== 'display' }" width="6cm" value="" items="{path: 'Model1>/PrevSixMonths', templateShareable:false}" change="onMonth" tooltip="{i18n>ClaimMonth}" class="sapUiMediumMarginEnd" >
                                 <items>
                                    <core:Item key="{Model1>key}" text="{Model1>months}" />
                                 </items>
                              </ComboBox>
                              <Label id="_IdGenLabel01" text="Claim End Date" required="true" showColon="true" width="3.5cm" class="sapUiTinyMarginTop sapUiLargeMarginBegin" labelFor="_IDGenDatePicker1" />
                              <DatePicker id="_IDGenDatePicker1" editable="false" class="sapUiLargeMarginEnd" width="4cm" change="onClaimEndDate" value="{Model1>/ClaimEndDate}" />
                           </HBox>
                           <!-- CLAIM MONTH, CLAIM END DATE -->
                        </VBox>
                     </Panel>
                     <!-- End Of Header -->
                     <!-- Begin Of Section A -->
                     <Panel id="_IDGenPanel3" headerText="Section A - Mileage Claims" expandable="true" expanded="true" width="auto" class="sapUiResponsiveMargin">
                        <Table id="_IDGenTable1" mode="MultiSelect" items="{Model1>/SecAData}" width="auto" >
                           <headerToolbar>
                              <OverflowToolbar id="_IDGenOverflowToolbar1">
                                 <content>
                                    <!--  <Title id="_IDGenTitle1" text="Section A - Claim Hours" level="H2"/>  -->
                                    <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                                    <ToggleButton id="toggleInfoToolbar1" text="Add Row" press="onAddRowTab1" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                    <ToggleButton id="toggleInfoToolbar2" text="Delete Row" press="onDelRowTab1" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                 </content>
                              </OverflowToolbar>
                           </headerToolbar>
                           <columns>
                              <Column id="_IDGenColumn1" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="6rem">
                                 <Label id="_IDGenLabel14" text="Emp.Name/Per no./Position" required="true" labelFor="_IDGenComboBox2" />
                              </Column>
                              <Column id="_IDGenColumn2" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel10" text="Vehicle Type" required="true" labelFor="_IDGenInput7" />
                              </Column>
                              <Column id="_IDGenColumn3" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel11" required="true" text="Vehicle Class" labelFor="_IDGenInput3" />
                              </Column>
                              <Column id="_IDGenColumn4" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="2rem">
                                 <Label id="_IDGenLabel12" text="No of Miles" labelFor="_IDGenInput4" />
                              </Column>
                              <Column id="_IDGenColumn41" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="2rem" visible="false" >
                                 <Label id="_IDGenLabel121" text="Amount" labelFor="_IDGenInput41" />
                              </Column>
                              <Column id="_IDGenColumn5" width="2rem" hAlign="Center">
                                 <Label id="_IDGenLabel13" text="" />
                              </Column>
                           </columns>
                           <items>
                              <ColumnListItem id="_IDGenColumnListItem1">
                                 <ComboBox id="_IDGenComboBox2" value="{Model1>empName}" enabled="{= ${Model1>/form/mode} !== 'display' }" items="{path: 'Model1>/dropdownEmp', templateShareable:false, length: 10000}" change="onEmplChangeSecA" tooltip="{i18n>empSel}" width="18rem">
                                    <items>
                                       <core:Item key="{Model1>userId}" text="{Model1>lastName} {Model1>firstName} {Model1>userId} {Model1>jobTitle}" bindingContexts="{Model1>jobTitle}" />
                                    </items>
                                 </ComboBox>
                                 <ComboBox id="_IDGenInput7" value="{Model1>VehicleType}" items="{path: 'Model1>/dropdownVehType', templateShareable:false}" change="onVehTypeChangeSecA" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableVehicleType}" tooltip="{i18n>vehicleType}" >
                                 <items>
                                       <core:Item key="{Model1>key}" text="{Model1>cust_VehicleType}" />
                                    </items>
                                 </ComboBox>
                                 <ComboBox id="_IDGenInput3" value="{Model1>VehicleClass}" items="{path: 'Model1>/dropdownVehClass', templateShareable:false}" change="onVehClassChangeSecA" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableVehicleClass}" tooltip="{i18n>vehicleClass}" >
                                 <items>
                                       <core:Item key="{Model1>cust_VehicleID}" text="{Model1>cust_VehicleClass}" />
                                    </items>
                                 </ComboBox>
                                 <Input id="_IDGenInput4" value="{Model1>NumberOfMiles}" type="Number"  change="onMilesSecA" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableMiles}" tooltip="{i18n>NoOfMiles}" />
                                 <Input id="_IDGenInput41" value="{Model1>Amount}" type="Number"  change="onAmountSecA" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableAmount}" visible="false" />
                                 <Button id="_IDGenButton1" type="Emphasized" text="Show" press="onShowPress1" tooltip="Add comments about this claim here" />
                              </ColumnListItem>
                           </items>
                        </Table>
                     </Panel>
                     <!-- End OF Section A -->
                     <!-- Begin Of Section B -->
                     <Panel id="_IDGenPanel4" headerText="Section B - Expenses Claims" expandable="true" expanded="true" width="auto" class="sapUiResponsiveMargin">
                        <Table id="_IDGenTable2" mode="MultiSelect" items="{Model1>/SecBData}" width="auto">
                           <headerToolbar>
                              <OverflowToolbar id="_IDGenOverflowToolbar2">
                                 <content>
                                    <!--  <Title id="_IDGenTitle1" text="Section A - Claim Hours" level="H2"/>  -->
                                    <ToolbarSpacer id="_IDGenToolbarSpacer2" />
                                    <ToggleButton id="toggleInfoToolbar3" text="Add Row" press="onAddRowTab2" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                    <ToggleButton id="toggleInfoToolbar4" text="Delete Row" press="onDelRowTab2" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                 </content>
                              </OverflowToolbar>
                           </headerToolbar>
                           <columns>
                              <Column id="_IDGenColumn6" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="8rem">
                                 <Label id="_IDGenLabel15" text="Emp.Name/Per no./Position" required="true" labelFor="_IDGenComboBox3" />
                              </Column>
                              <Column id="_IDGenColumn7" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="5rem">
                                 <Label id="_IDGenLabel16" text="Expense Type" required="true" labelFor="_IDGenInput8" />
                              </Column>
                              <Column id="_IDGenColumn8" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="3rem">
                                 <Label id="_IDGenLabel17" text="Amount(£)" labelFor="_IDGenInput9" />
                              </Column>
                              <Column id="_IDGenColum9" width="2rem" hAlign="Center">
                                 <Label id="_IDGenLabel18" text="" />
                              </Column>
                           </columns>
                           <items>
                              <ColumnListItem id="_IDGenColumnListItem2">
                                 <ComboBox id="_IDGenComboBox3" value="{Model1>empName}" enabled="{= ${Model1>/form/mode} !== 'display' }" items="{path: 'Model1>/dropdownEmp', templateShareable:false, length: 10000}" change="onEmplChangeSecB" tooltip="{i18n>empSel}" width="22rem">
                                    <items>
                                       <core:Item key="{Model1>userId}" text="{Model1>lastName} {Model1>firstName} {Model1>userId} {Model1>jobTitle}" />
                                    </items>
                                 </ComboBox>
                                 <ComboBox id="_IDGenInput8" items="{path: 'Model1>/dropdownExpType', templateShareable:false }" value="{Model1>ExpenseType}" change="onExpenseChangeSecB" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableExpense}" tooltip="{i18n>ExpenseType}" >
                                    <items>
                                       <core:Item key="{Model1>cust_ExpenseType}" text="{Model1>cust_ExpenseID}" />
                                    </items>
                                 </ComboBox>
                                 <Input id="_IDGenInput9" value="{Model1>Amount}" type="Number" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableAmount}" tooltip="{i18n>Amount}" />
                                 <Button id="_IDGenButton2" type="Emphasized" text="Show" press="onShowPress1" tooltip="Add comments about this claim here" />
                              </ColumnListItem>
                           </items>
                        </Table>
                     </Panel>
                     <!-- End Of Section B -->
                     <!-- Begin Of Section C -->
                     <Panel id="_IDGenPanel5" headerText="Section C - Supply Mileage" expandable="true" expanded="true" width="auto" class="sapUiResponsiveMargin">
                        <Table id="_IDGenTable3" mode="MultiSelect" items="{Model1>/SecCData}" width="auto">
                           <headerToolbar>
                              <OverflowToolbar id="_IDGenOverflowToolbar3">
                                 <content>
                                    <!--  <Title id="_IDGenTitle1" text="Section A - Claim Hours" level="H2"/>  -->
                                    <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                                    <ToggleButton id="toggleInfoToolbar5" text="Add Row" press="onAddRowTab3" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                    <ToggleButton id="toggleInfoToolbar6" text="Delete Row" press="onDelRowTab3" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                 </content>
                              </OverflowToolbar>
                           </headerToolbar>
                           <columns>
                              <Column id="_IDGenColumn10" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="3rem">
                                 <Label id="_IDGenLabel19" text="Personal Number" required="true" labelFor="_IDGenInput10" />
                              </Column>
                              <Column id="_IDGenColumn11" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel20" text="Employee Name" required="true" labelFor="_IDGenInput11" />
                              </Column>
                              <Column id="_IDGenColumn12" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel21" text="Vehicle Type" labelFor="_IDGenInput12" />
                              </Column>
                              <Column id="_IDGenColumn13" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel22" text="Vehicle Class" labelFor="_IDGenInput13" />
                              </Column>
                              <Column id="_IDGenColumn14" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="2rem">
                                 <Label id="_IDGenLabel23" text="No of Miles" labelFor="_IDGenInput14" />
                              </Column>
                              <Column id="_IDGenColumn141" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="2rem" visible="false" >
                                 <Label id="_IDGenLabel231" text="Amount" labelFor="_IDGenInput141" />
                              </Column>
                              <Column id="_IDGenColum15" width="2rem" hAlign="Center">
                                 <Label id="_IDGenLabel24" text="" />
                              </Column>
                           </columns>
                           <items>
                              <ColumnListItem id="_IDGenColumnListItem3">
                                 <Input id="_IDGenInput10" value="{Model1>PersonID}" enabled="{= ${Model1>/form/mode} !== 'display' }" change="onIDChange" tooltip="{i18n>PersonnelNo}" />
                                 <Input id="_IDGenInput11" value="{Model1>empName}" editable="false" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                 <ComboBox id="_IDGenInput71" value="{Model1>VehicleType}" items="{path: 'Model1>/dropdownVehType', templateShareable:false}" change="onVehTypeChangeSecA" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableVehicleType}" tooltip="{i18n>vehicleType}" >
                                 <items>
                                       <core:Item key="{Model1>key}" text="{Model1>cust_VehicleType}" />
                                    </items>
                                 </ComboBox>
                                 <ComboBox id="_IDGenInput31" value="{Model1>VehicleClass}" items="{path: 'Model1>/dropdownVehClass', templateShareable:false}" change="onVehClassChangeSecA" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableVehicleClass}" tooltip="{i18n>vehicleClass}" >
                                 <items>
                                       <core:Item key="{Model1>cust_VehicleID}" text="{Model1>cust_VehicleClass}" />
                                    </items>
                                 </ComboBox>
                                 <Input id="_IDGenInput14" value="{Model1>NumberOfMiles}" type="Number" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableMiles}" tooltip="{i18n>NoOfMiles}" />
                                 <Input id="_IDGenInput141" value="{Model1>Amount}" type="Number" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableAmount}" visible="false" />
                                 <Button id="_IDGenButton3" type="Emphasized" text="Show" press="onShowPress1" tooltip="Add comments about this claim here" />
                              </ColumnListItem>
                           </items>
                        </Table>
                     </Panel>
                     <!-- End Of Section C -->
                     <!-- Begin Of Section D -->
                     <Panel id="_IDGenPanel6" headerText="Section D - Supply Expense" expandable="true" expanded="true" width="auto" class="sapUiResponsiveMargin">
                        <Table id="_IDGenTable4" mode="MultiSelect" items="{Model1>/SecDData}" width="auto">
                           <headerToolbar>
                              <OverflowToolbar id="_IDGenOverflowToolbar4">
                                 <content>
                                    <!--  <Title id="_IDGenTitle1" text="Section A - Claim Hours" level="H2"/>  -->
                                    <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                                    <ToggleButton id="toggleInfoToolbar7" text="Add Row" press="onAddRowTab4" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                    <ToggleButton id="toggleInfoToolbar8" text="Delete Row" press="onDelRowTab4" enabled="{= ${Model1>/form/mode} !== 'display' }" />
                                 </content>
                              </OverflowToolbar>
                           </headerToolbar>
                           <columns>
                              <Column id="_IDGenColumn16" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="3rem">
                                 <Label id="_IDGenLabel25" text="Personal Number" required="true" labelFor="_IDGenInput15" />
                              </Column>
                              <Column id="_IDGenColumn17" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel26" text="Employee Name" required="true" labelFor="_IDGenInput16" />
                              </Column>
                              <Column id="_IDGenColumn18" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="4rem">
                                 <Label id="_IDGenLabel27" text="Expense Type" labelFor="_IDGenInput17" />
                              </Column>
                              <Column id="_IDGenColumn19" minScreenWidth="Tablet" demandPopin="true" hAlign="Center" width="3rem">
                                 <Label id="_IDGenLabel28" text="Amount(£)" labelFor="_IDGenInput18" />
                              </Column>
                              <Column id="_IDGenColumn20" width="2rem" hAlign="Center">
                                 <Label id="_IDGenLabel29" text="" />
                              </Column>
                           </columns>
                           <items>
                              <ColumnListItem id="_IDGenColumnListItem4">
                                 <Input id="_IDGenInput15" value="{Model1>PersonID}" enabled="{= ${Model1>/form/mode} !== 'display' }" change="onIDChangeSecD" tooltip="{i18n>PersonnelNo}" />
                                 <Input id="_IDGenInput16" value="{Model1>empName}" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="false" />
                                  <ComboBox id="_IDGenInput81" items="{path: 'Model1>/dropdownExpType', templateShareable:false}" value="{Model1>ExpenseType}" change="onExpenseChangeSecB" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableExpense}" tooltip="{i18n>ExpenseType}" >
                                    <items>
                                       <core:Item key="{Model1>cust_ExpenseType}" text="{Model1>cust_ExpenseID}" />
                                    </items>
                                 </ComboBox>
                                 <Input id="_IDGenInput18" value="{Model1>Amount}" type="Number" enabled="{= ${Model1>/form/mode} !== 'display' }" editable="{Model1>enableAmount}" tooltip="{i18n>Amount}" />
                                 <Button id="_IDGenButton4" type="Emphasized" text="Show" press="onShowPress1" tooltip="Add comments about this claim here" />
                              </ColumnListItem>
                           </items>
                        </Table>
                     </Panel>
                     <!-- End Of Section D -->
                     <!-- Begin of Comments -->
                     <Panel id="_IDGenPanel7" expandable="true" expanded="true" width="auto" stickyHeader="true" headerText="{i18n>Comments}" class="sapUiResponsiveMargin">
                        <VBox id="_IDGenVBox2">
                           <Label id="_IDGenLabe20" text="{i18n>AuthComments}" showColon="true" visible="false" />
                           <TextArea id="_IDGenTextArea2" width="1000px" height="150px" visible="false" enabled="false" />
                           <Label id="_IDGenLabel4" text="Your Comments" showColon="true" class="sapUiSmallMarginTop" />
                           <TextArea id="_IDGenTextArea3" width="1000px" height="60px" growing="true" growingMaxLines="5" tooltip="{i18n>yourComments}" />
                           <Button id="button1" text="Add Comment" press="onAddComment" type="Emphasized" />
                        </VBox>
                     </Panel>
                     <!-- End of Comments -->
                     <!-- Begin of Section E -->
                     <Panel id="_IDGenPanel8" headerText="Section E - Notify Approver" expandable="true" expanded="true" class="sapUiResponsiveMargin" width="auto">
                        <content>
                           <CheckBox id="checkbox1" enabled="{= ${Model1>/form/mode} !== 'display' }" text="{i18n>checkBoxText}" />
                           <MessageStrip id="_IDGenMessageStrip1" text="{i18n>message}" />
                        </content>
                     </Panel>
                     <!-- End of Section E -->
                  </VBox>
               </IconTabFilter>
               <IconTabFilter id="_IDGenIconTabFilter2" text="History" key="History">
                  <Panel id="_IDGenPanel9" expandable="false" stickyHeader="true" headerText="Expense Report Form" width="auto" class="sapUiResponsiveMargin" >
                        <Table id="_IDGenTable5" width="auto" delete="deleteRow" mode="MultiSelect" items="{Model1>/historyTable}">
                            <headerToolbar>
                                <Toolbar id="_IDGenToolbar5" width="auto" design="Solid" style="Standard">
                                    <Label id="_HIDGenLabel1" labelFor="_HIDGenFormId1" text="Form Id"/>
                                    <Input id="_HIDGenFormId1" width="auto" enabled="false"/>
                                    <Label id="_HIDGenLabel2" labelFor="_HIDGenType1" text="Type"/>
                                    <Input id="_HIDGenType1" width="auto" enabled="false" value="EX01"/>
                                </Toolbar>
                            </headerToolbar>
                            <columns>
                                <Column id="_HIDGenColumn1" demandPopin="true" minScreenWidth="Tablet" width="80px">
                                    <Text id="_HIDGenText1" text="{i18n>HCol1}"/>
                                </Column>
                                    <Column id="_HIDGenColumn2" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText2" text="{i18n>HCol2}"/>
                                </Column>
                                <Column id="_HIDGenColumn3" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText3" text="{i18n>HCol3}"/>
                                </Column>
                                <Column id="_HIDGenColumn4" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText4" text="{i18n>HCol4}"/>
                                </Column>
                                <Column id="_HIDGenColumn5" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText5" text="{i18n>HCol5}"/>
                                </Column>
                                <Column id="_HIDGenColumn6" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText6" text="{i18n>HCol6}"/>
                                </Column>
                                <Column id="_HIDGenColumn7" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText7" text="{i18n>HCol7}"/>
                                </Column>
                                <Column id="_HIDGenColumn8" demandPopin="true" minScreenWidth="Tablet">
                                    <Text id="_HIDGenText8" text="{i18n>HCol8}"/>
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem id="_IDGenColumnListItem6">
                                    <cells>
                                        <Input id="_HIDGenInput1" editable="false" value="{Model1>SeqNumber}" tooltip="Sequence Number" width="60px" />
                                        <Input id="_HIDGenInput2" editable="false" value="{Model1>Initiator}" tooltip="Initiator" />
                                        <Input id="_HIDGenInput3" editable="false" value="{Model1>StartedOn}" tooltip="Started On" />
                                        <Input id="_HIDGenInput4" editable="false" value="{Model1>OrganizationName}" tooltip="Organization Name" />
                                        <Input id="_HIDGenInput5" editable="false" value="{Model1>Description}" tooltip="Description" />
                                        <Input id="_HIDGenInput6" editable="false" value="{Model1>Status}" tooltip="Status" />
                                        <Input id="_HIDGenInput7" editable="false" value="{Model1>FormOwner}" tooltip="Form Owner" />
                                        <Input id="_HIDGenInput8" editable="false" value="{Model1>AvailableFrom}" tooltip="Available From" />
                                        <!-- <Input id="_HIDGenInput9" editable="false" value="{AllData>createdOn}" tooltip="Created On" /> -->
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                    </Panel>
               </IconTabFilter>
            </items>
         </IconTabBar>
      </content>
